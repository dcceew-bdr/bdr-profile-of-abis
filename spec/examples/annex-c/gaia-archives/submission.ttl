PREFIX prof: <http://www.w3.org/ns/dx/prof/>
PREFIX schema: <https://schema.org/>
PREFIX smm: <https://linked.data.gov.au/def/bdr-pr/smm/>
PREFIX smmrr: <https://linked.data.gov.au/def/bdr-pr/smmrr/>

[]
    a smm:SubmissionManifest ;
    prof:hasResource
        [
            prof:hasArtifact
                "outputs/dataset.ttl" ,
                "outputs/survey-metadata.ttl" ;
            prof:hasRole smmrr:NecessaryForValidation ;
        ] ,
        [
            prof:hasArtifact "outputs/chunk_*.ttl" ;
            prof:hasRole smmrr:IsolatedContent ;
        ] ,
        [
            # there must be a reference to this content in the RDF:
            # {DATASET-IRI} prov:wasDerivedFrom "inputs/"
            # which we will replace "input/" with an actual BDR archival
            # location, when created
            prof:hasArtifact "inputs/" ;
            prof:hasRole smmrr:SubmissionSourceArchive ;
        ] ;
.
